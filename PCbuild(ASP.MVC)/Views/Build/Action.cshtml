@model IEnumerable<PCbuild_ASP.MVC_.Models.BuildResult>

<div class="row">

    @{ foreach (var m in Model)
        {
    <div class="col-sm-6 col-md-4">
        <div class="thumbnail" style="width: 18rem;">
            <img src="@Url.Action("GetImage","Build" , new { m.Game.GameID, big=true })"/>
            <div class="caption">

                <p>@m.Game.Name</p>
                <p>@m.FPS FPS</p>

            </div>
        </div>
    </div>
                }
                }
            
        </div>
