@using PCbuilder_ASP.MVC_.HtmlHelpers;
@model PCbuilder_ASP.MVC_.Models.CPUListViewModel

@{
    ViewBag.Title = "CPUs";
}

<h2>ListCPU</h2>

<div class="showmenu">
    @{
        Html.RenderPartial("ShowMenu","CPU");
    }
</div>

<div>
    @foreach(var c in Model.CPUs)
    {
        Html.RenderPartial("CPUSummary", c);
    }
</div>

<div class="pagination">
    @Html.PageLinks(Model.PagingInfo, x=>Url.Action("ListCPU",new { page = x}))
</div>